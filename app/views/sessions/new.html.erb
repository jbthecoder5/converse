<div class="page-header">
<h1>Log In</h1>
</div>
 <%= form_tag sessions_path do %>
<div>
	<%= label_tag :email %>
	<%= email_field_tag :email, params[:email],
	class: "form-control" %>
</div>
<div>
	<%= label_tag :password %>
	<%= password_field_tag :password, nil,
	class: "form-control" %>
</div>
<p>
	<%= submit_tag "Log In", class: "btn btn-primary" %>
	<%= link_to 'new user?', signup_path %>
</p>
<% end %>